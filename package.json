    "pinp": "^1.4.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.7.2",
    "wrangler": "^3.95.0"
